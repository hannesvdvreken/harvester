# 8 arrival_delay
# 9 date

<#> a :Spec
; :format :CSV
; :header_rows 0
; :resource <#ServiceStop>, <#Stop>, <#Service>
; :base_uri "http://data.iRail.be/NMBS/"
.

<#ServiceStop> a :Resource
; :type transit:serviceStop
; :identity [
	:source_columns ( 2 9 1 );
	:source_column_glue "/" ;
	:base_uri "http://data.iRail.be/NMBS/ServiceStops/"
	]
; :relationship
	[ :property transit:stop; :object_from <#Stop> ],
	[ :property transit:service, :object_from <#Service> ]
; :attribute
	[ :property transit:sequence; :source_column 1 ],
	[ :property transit:arrivalTime; :source_column 6 ],
	[ :property transit:departureTime; :source_column 5 ],
	[ :property transit:headsign; :source_column 4 ],
	[ :property irail:departureDelay; :source_column 7 ],
	[ :property irail:arrivalDelay; :source_column 8 ]
.

<#Stop> a :Resource
; :type transit:stop
; :identity [
	:source_column 3;
	:base_uri "http://data.iRail.be/NMBS/Stops"
]
.

<#Service> a :Resource
; :type transit:service
; :identity [
	:source_column 2;
	:base_uri "http://data.iRail.be/NMBS/Services"
	]
.